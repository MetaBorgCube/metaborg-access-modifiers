module statics/base

signature

  name-resolution
	labels
	  LEX 					// lexical parent
	  IMPORT 				// import
	  EXTENDS				// extends (across module boundary)
      EXTENDS_INTERNAL      // extends (same package)

      PARENT				// distinguish scoped extends from local fields

	  // Labels for visibility levels
      PROTECTED
      INTERNAL
      PROTECTED_INTERNAL
      PRIVATE
      PRIVATE_INTERNAL

  sorts TYPE constructors
    INT		: TYPE
    CLASS 	: scope -> TYPE
