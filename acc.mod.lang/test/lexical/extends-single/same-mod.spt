module lexical/extends-single/same-mod

test private [[
module p {
  class A {
    public var x = 42
  }
  class B : private A { }
  class C : public B {
    private var y = x
  }
}
]]
analysis fails

test private internal [[
module p {
  class A {
    public var x = 42
  }
  class B : private internal A {
  }
  class C : public B {
    private var y = x
  }
}
]]
analysis succeeds

