module lexical/single-other-mod

test private [[
module p {
  class A {
    private var x = 42
  }
}
module q {
  import p
  class B : private A {
    private var y = x
  }
}
]]
analysis fails

test private internal [[
module p {
  class A {
    private internal var x = 42
  }
}
module q {
  import p
  class B : private A {
    private var y = x
  }
}
]]
analysis fails

test internal [[
module p {
  class A {
    internal var x = 42
  }
}
module q {
  import p
  class B : private A {
    private var y = x
  }
}
]]
analysis fails

test protected internal [[
module p {
  class A {
    protected internal var x = 42
  }
}
module q {
  import p
  class B : private A {
    private var y = x
  }
}
]]
analysis succeeds

