module lexical/single-same-mod

test private [[
module p {
  class A {
    private var x = 42
  }
  class B : private A {
    private var y = x
  }
}
]]
analysis fails

test private internal [[
module p {
  class A {
    private internal var x = 42
  }
  class B : private A {
    private var y = x
  }
}
]]
analysis succeeds

test internal [[
module p {
  class A {
    internal var x = 42
  }
  class B : private A {
    private var y = x
  }
}
]]
analysis succeeds

test protected internal [[
module p {
  class A {
    protected internal var x = 42
  }
  class B : private A {
    private var y = x
  }
}
]]
analysis succeeds

test protected [[
module p {
  class A {
    protected var x = 42
  }
  class B : private A {
    private var y = x
  }
}
]]
analysis succeeds

test public [[
module p {
  class A {
    public var x = 42
  }
  class B : private A {
    private var y = x
  }
}
]]
analysis succeeds


