module lexical/extends-multi/same-mod

test private [[
module p {
  class A {
    public var x = 42
  }
  class B : public A { }
  class C : private B { }
  class D : public C {
    private var y = x
  }
}
]]
analysis fails

test private internal [[
module p {
  class A {
    public var x = 42
  }
  class B : public A { }
  class C : private internal B { }
  class D : public C {
    private var y = x
  }
}
]]
analysis succeeds

