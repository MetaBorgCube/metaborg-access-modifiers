module lexical/extends-multi/other-mod

test private [[
module p {
  class A {
    public var x = 42
  }
}
module q {
  import p
  class B : public A { }
  class C : private B {
  }
  class D : public C {
    private var y = x
  }
}
]]
analysis fails

