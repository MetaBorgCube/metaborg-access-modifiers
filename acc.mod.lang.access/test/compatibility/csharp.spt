module compatibility/csharp

test simple [[
module M1 {
class A {
  protected internal var x = 42
}
}
module M2 {
import M1
class B : public A {
  public var a = new A()
}
}
]]
run csharp-test-transform
transform "Test C# Compatibility"

test nested - refer to parent [[
module M1 {
  class A {
    protected internal var x = 42
  }
  module M2 {
    class B : public A {
      public var a = new A()
    }
  }
}
]]
run csharp-test-transform
transform "Test C# Compatibility"

test nested - refer to root [[
