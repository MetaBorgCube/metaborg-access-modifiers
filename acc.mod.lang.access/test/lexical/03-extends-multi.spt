module lexical/extends-multi

test same mod - private [[
module p {
  class A {
    public var x = 42
  }
  class B : public A { }
  class C : private B { }
  class D : public C {
    private var y = x
  }
}
]]
analysis fails


test same mod - protected [[
module p {
  class A {
    public var x = 42
  }
  class B : public A { }
  class C : protected B { }
  class D : public C {
    private var y = x
  }
}
]]
analysis succeeds

